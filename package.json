{
  "name": "nest-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NEST_DEBUG=true nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "pretest:e2e": "docker-compose up -d test-db",
    "posttest:e2e": "docker-compose stop test-db && docker-compose rm -f test-db",
    "threat-model": "ts-node -r tsconfig-paths/register src/threat-modelling/cli.ts",
    "threat-model:build": "nest build && node dist/src/threat-modelling/cli.js"
  },
  "dependencies": {
    "@google/genai": "^0.7.0",
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "11.0.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.12",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/swagger": "11.1.0",
    "@nestjs/typeorm": "11.0.0",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "install": "0.13.0",
    "pg": "8.14.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.2",
    "@nestjs/testing": "11.0.12",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.13",
    "@types/express": "5.0.1",
    "@types/hapi__joi": "17.1.15",
    "@types/node": "22.13.14",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "7.0.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/coverage-v8": "2.0.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.3",
    "prettier": "3.3.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "unplugin-swc": "1.4.5",
    "vitest": "2.1.9"
  }
}
